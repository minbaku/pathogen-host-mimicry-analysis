from src.peptide_utils importload_sequences, generate_peptides

seqs = load_sequences("..data/raw/sequences.fasta")

pathogen_peptides = generate_peptides(seqs["pathogen_protein"], k=9)
human_peptides = generate_peptides(seqs["Human_protein"], k=9)

len(pathogen_peptides, len(human_peptides)

def exact_matches(peptides_a, peptides_b):
  return set(peptides_a).intersection(set(peptides_b))

matches = exact_matches(pathogen_peptides, human_peptides)
matches
